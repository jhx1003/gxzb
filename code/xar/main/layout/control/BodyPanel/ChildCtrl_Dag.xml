<xlue>
	<control class="ChildCtrl_Dag">
		<attr_def>
			<attr name="bHasInit" type="bool"/>
		</attr_def>
		<method_def>
			
		</method_def>
		<event_def>
			
		</event_def>
		<objtemplate>
			<children>
				<obj id="ChildCtrl_Dag.UserInfo" class="LayoutObject">
					<attr>
						<left>16</left>
						<top>16</top>
						<width>132</width>
						<height>father.height</height>
					</attr>
					<children>
						<obj id="ChildCtrl_Dag.UserInfo.HeadImg" class="ImageObject">
							<attr>
								<left>0</left>
								<top>0</top>
								<width>132</width>
								<height>132</height>
								<image>WeiXin.HeadImg.default</image>
							</attr>
						</obj>
						<obj id="ChildCtrl_Dag.UserInfo.NickName" class="TextObject">
							<attr>
								<left>0</left>
								<top>132+5</top>
								<width>132</width>
								<height>20</height>
								<text>小胜六步</text>
								<font>font.text15</font>
								<textcolor>system.black</textcolor>
								<halign>center</halign>
								<endellipsis>1</endellipsis>
							</attr>
						</obj>
					</children>
				</obj>
				<obj id="ChildCtrl_Dag.Cycle" class="TextureObject">
					<attr>
						<left>(father.width - 184)/2</left>
						<top>30</top>
						<width>184</width>
						<height>184</height>
						<texture>GXZB.MainPanel.Cycle</texture>
					</attr>
					<children>
						<obj id="ChildCtrl_Cash.Text" class="TextObject">
							<attr>
								<left>(184-130)/2</left>
								<top>(184-40)/2</top>
								<width>130</width>
								<height>40</height>
								<text>正在初始化</text>
								<halign>center</halign>
								<font>font.text15</font>
								<textcolor>system.black</textcolor>
							</attr>
						</obj>
					</children>
				</obj>
			</children>
		</objtemplate>
	</control>	
</xlue>