<xlue>
	<objtreetemplate id="GXZB.MachineCheckWndTree" class="ObjectTreeTemplate">
		<attr>
			<left>0</left>
			<top>0</top>
			<width>500</width>
			<height>280</height>
		</attr>
		<obj id="root" class="TextureObject">
			<attr>
				<left>0</left>
				<top>0</top>
				<width>500</width>
				<height>280</height>
				<texture>common-bkg</texture>
			</attr>	
			<children>
				<obj id="MachineCheckWnd.Caption" class="CaptionObject">
					<attr>
						<left>0</left>
						<top>0</top>
						<width>father.width</width>
						<height>father.height</height>
					</attr>
					<children>
						<obj id="MachineCheckWnd.TitleBar" class="LayoutObject">
							<attr>
								<left>0</left>
								<top>0</top>
								<width>father.width</width>
								<height>30</height>
							</attr>
							<children>
								<obj id="MachineCheckWnd.TitleBar.Logo" class="ImageObject">
									<attr>
										<left>10</left>
										<top>5</top>
										<width>14</width>
										<height>13</height>
										<image>GXZB.Caption.Logo</image>
									</attr>
								</obj>
								<obj id="MachineCheckWnd.TitleBar.Title" class="TextObject">
									<attr>
										<left>10+20+5</left>
										<top>0</top>
										<width>200</width>
										<height>father.height</height>
										<text>共享赚宝</text>
										<textcolor>system.black</textcolor>
										<font>font.default12</font>
										<valign>center</valign>
									</attr>
								</obj>
								<obj id="MachineCheckWnd.TitleBar.CloseBtn" class="TipAddin.Button">
									<attr>
										<left>father.width-25</left>
										<top>8</top>
										<width>15</width>
										<height>15</height>
										<NormalBkgID>GXZB.Caption.BtnClose.Normal</NormalBkgID>
										<DownBkgID>GXZB.Caption.BtnClose.Down</DownBkgID>
										<HoverBkgID>GXZB.Caption.BtnClose.Hover</HoverBkgID>
										<DisableBkgID>GXZB.Caption.BtnClose.Normal</DisableBkgID>
									</attr>
									<eventlist>
										<event name="OnClick" file="MachineCheckWnd.xml.lua" func="OnClickClose" />
										<event name="OnMouseEnter">
											local self = ...
											local hoverimage = self:GetObject("hoverimage")
											if not hoverimage then
												local objFactory = XLGetObject("Xunlei.UIEngine.ObjectFactory")
												hoverimage = objFactory:CreateUIObject("hoverimage", "ImageObject")
												self:AddChild(hoverimage)
												hoverimage:SetObjPos(-5, -9, 20, -6)
												hoverimage:SetResID("GXZB.Title.Hoverimage")
											end
											hoverimage:SetVisible(true)
										</event>
										<event name="OnMouseLeave">
											local self = ...
											local hoverimage = self:GetObject("hoverimage")
											if hoverimage then
												hoverimage:SetVisible(false)
											end
										</event>
									</eventlist>	
								</obj>
							</children>
						</obj>
						<obj id="MachineCheckWnd.Content" class="LayoutObject">
							<attr>
								<left>0</left>
								<top>30</top>
								<width>father.width</width>
								<height>father.height-30</height>
							</attr>
							<children>
								<obj id="MachineCheckWnd.Content.Warning" class="ImageObject">
									<attr>
										<left>95</left>
										<top>65</top>
										<width>35</width>
										<height>35</height>
										<image>logo-mid</image>
										<drawmode>1</drawmode>
									</attr>
								</obj>
								<obj id="MachineCheckWnd.Content.Text" class="TextObject">
									<attr>
										<left>95+35+10</left>
										<top>65+5</top>
										<width>300</width>
										<height>24</height>
										<valign>center</valign>
										<halign>left</halign>
										<textcolor>DDDDDD</textcolor>
										<font>font.text18</font>
										<text>软件检测到本设备不支持赚宝</text>
									</attr>
								</obj>
								<obj id="MachineCheckWnd.Content.ExitBtn" class="TipAddin.Button">
									<attr>
										<Text>退出</Text>
										<TextFont>font.text16</TextFont>
										<TextColor>DDDDDD</TextColor>
										<left>(father.width-73)/2</left>
										<top>164</top>
										<width>90</width>
										<height>30</height>
										<NormalBkgID>setting-btn-normal</NormalBkgID>
										<DownBkgID>setting-btn-normal</DownBkgID>
										<HoverBkgID>setting-btn-hover</HoverBkgID>
										<DisableBkgID>setting-btn-hover</DisableBkgID>
									</attr>
									<eventlist>
										<event name="OnClick" file="MachineCheckWnd.xml.lua" func="OnClickExit" />
									</eventlist>	
								</obj>
								<obj id="MachineCheckWnd.Content.Share" class="TextLinkEx">
									<attr>
										<left>father.width-73-18</left>
										<top>father.height-18-18</top>
										<width>77</width>
										<height>18</height>
										<HAlign>center</HAlign>
										<VAlign>center</VAlign>
										<Text>分享赚分成></Text>
										<Font>font.text13</Font>
										<TextColor>C4C400</TextColor>
										<TextHoverColor>C4C400</TextHoverColor>
										<Underline>false</Underline>
									</attr>
									<eventlist>
										<event name="OnClick" file="MachineCheckWnd.xml.lua" func="OnClickShare" />
									</eventlist>
								</obj>
							</children>
						</obj>
					</children>
				</obj>
			</children>
		</obj>
	</objtreetemplate>

    <hostwndtemplate id="GXZB.MachineCheckWnd" class="FrameHostWnd">
        <attr>
            <layered>1</layered>
            <left>0</left>
            <top>0</top>
            <width>500</width>
			<height>280</height>
            
            <topmost>0</topmost>
            <visible>1</visible>
            <enable>1</enable>
            <maxbox>0</maxbox>
            <minbox>1</minbox>
            <minwidth>50</minwidth>
            <minheight>50</minheight>
			<sysmenu>0</sysmenu>
			<appwindow>1</appwindow>
            <fps>30</fps>
			<blur>1</blur>
        </attr>
		 <eventlist>
			<event name="OnShowWindow" file="MachineCheckWnd.xml.lua" func="OnShowWindow" />
        </eventlist>
    </hostwndtemplate>
</xlue>
