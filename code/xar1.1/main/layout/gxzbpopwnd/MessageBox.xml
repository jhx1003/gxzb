<xlue>
<objtreetemplate id="MessageBoxWndTree" class="ObjectTreeTemplate">
	<obj id="PanelCaption" class="CaptionObject">
		<attr>
			<top>0</top>
			<left>0</left>
			<width>470</width>
			<height>270</height>
		</attr>
		<children>
			<obj id="root" class="TextureObject">
				<attr>
					<left>0</left>
					<top>0</top>
					<width>father.width</width>
					<height>father.height</height>
					<texture>common-bkg</texture>
				</attr>	
				<children>
					<obj id="close" class="TipAddin.Button">
						<attr>
							<left>father.width-30</left>
							<top>5</top>
							<height>12</height>
							<width>12</width>
							<NormalBkgID>GXZB.Caption.BtnClose.Normal</NormalBkgID>
							<DownBkgID>GXZB.Caption.BtnClose.Down</DownBkgID>
							<HoverBkgID>GXZB.Caption.BtnClose.Hover</HoverBkgID>
							<DisableBkgID>GXZB.Caption.BtnClose.Normal</DisableBkgID>
						</attr>
						<eventlist>
							<event name="OnClick" file="MessageBox.xml.lua" func="closeOnClick" />
						</eventlist>
					</obj>
					<obj id="titlehead" class="TextObject">
						<attr>
							<left>12</left>
							<top>0</top>
							<height>28</height>
							<width>90</width>
							<textcolor>system.white</textcolor>
							<text></text>
							<halign>left</halign>
							<font>font.text13</font>
							<valign>center</valign>
						</attr>
					</obj>

					<obj id="MainText" class="TextObject">
						<attr>
							<left>173</left>
							<top>120</top>
							<width>father.width-200</width>
							<height>father.height-36</height>
							<multiline>1</multiline>
							<font>font.text14</font>
							<text></text>
							<halign>left</halign>
							<textcolor>system.white</textcolor>
							<wordellipsis>1</wordellipsis>
							<valign>left</valign>
							
						</attr>
					</obj>
					<obj id="yes" class="TipAddin.Button">
						<attr>
							<left>159</left>
							<top>198</top>
							<height>25</height>
							<width>73</width>
							<NormalBkgID>setting-btn-normal</NormalBkgID>
							<DownBkgID>setting-btn-normal</DownBkgID>
							<HoverBkgID>setting-btn-hover</HoverBkgID>
							<DisableBkgID>setting-btn-hover</DisableBkgID>
							<Text>可以</Text>
							<TextFont>font.text16</TextFont>
							<TextColor>DDDDDD</TextColor>

						</attr>
						<eventlist>
							<event name="OnClick" file="MessageBox.xml.lua" func="yesOnClick" />
						</eventlist>
					</obj>
					<obj id="no" class="TipAddin.Button">
						<attr>
							<left>241</left>
							<top>198</top>
							<height>25</height>
							<width>73</width>
							<NormalBkgID>setting-btn-normal</NormalBkgID>
							<DownBkgID>setting-btn-normal</DownBkgID>
							<HoverBkgID>setting-btn-hover</HoverBkgID>
							<DisableBkgID>setting-btn-hover</DisableBkgID>
							<Text>不行</Text>
							<TextFont>font.text16</TextFont>
							<TextColor>DDDDDD</TextColor>
						</attr>
						<eventlist>
							<event name="OnClick" file="MessageBox.xml.lua" func="noOnClick" />
						</eventlist>
					</obj>
					<obj id="MainIcon" class="ImageObject">
						<attr>
							<left>110</left>
							<top>110</top>
							<height>50</height>
							<width>50</width>
							<image>logo-mid</image>
						</attr>
					</obj>
				</children>
			</obj>
		</children>
	</obj>
</objtreetemplate>

 <hostwndtemplate id="MessageBoxWnd" class="ModalHostWnd">
	<attr>
		<layered>1</layered>
		<left>0</left>
		<top>0</top>
		<width>430</width>
		<height>150</height>
		
		<topmost>0</topmost>
		<visible>1</visible>
		<enable>1</enable>
		<maxbox>1</maxbox>
		<minbox>1</minbox>
		<minwidth>50</minwidth>
		<minheight>50</minheight>
		<sysmenu>0</sysmenu>
		<appwindow>0</appwindow>
	</attr>
	 <eventlist>
		<event name="OnCreate" file="MessageBox.xml.lua" func="OnCreate"/>
	</eventlist>
</hostwndtemplate>
</xlue>